-- fruit

fruits = {}

function number_fruits()
	local n = 0

    for fruit in all(fruits) do
		if(fruit.act == true) then
			n += 1
		end
    end

	return n
end

function check_fruit_collision(x, y)
    for fruit in all(fruits) do
		if(fruit.act == true) then
			if((x == fruit.x) and (y == fruit.y)) then
				return fruit
			end
		end
    end
    return nil
end

function respawn_fruit(fruit)
	if(((grid_size_x * grid_size_y) - (number_fruits() + cobra.size)) < 0) then
		fruit.act = false
		return
	end

	local valid = false

	while(not (valid)) do
		fruit.x = (offset_x + (flr(rnd(grid_size_x)) * 8))
		fruit.y = (offset_y + (flr(rnd(grid_size_y)) * 8))

		valid = true
		local aux = cobra.head
		while(aux != nil) do
			if((aux.x == fruit.x) and (aux.y == fruit.y))then
				valid = false
				break
			end
			aux = aux.next
		end
		
		local tile_x = flr(fruit.x / 8) + map_start_x
        local tile_y = flr(fruit.y / 8)

        if(fget(mget(tile_x, tile_y), 0)) then
            valid = false
        end

		for other_fruit in all(fruits) do
			if(other_fruit.act == true) then
				if(other_fruit != fruit) then
					if((other_fruit.x == fruit.x) and (other_fruit.y == fruit.y)) then
						valid = false
						break
					end
				end
			end
        end
	end
end

function init_fruit()
	while(#fruits > 0) do
		del(fruits, fruits[1])
	end

    local max_fruits = fruit_count_index
    
    for i = 1, max_fruits do
        add(fruits, {spr = 16, act = true})
        respawn_fruit(fruits[i])
    end
end

function update_fruit()

end

function draw_fruit()
	for fruit in all(fruits) do
		if(fruit.act == true) then
        	spr(fruit.spr, fruit.x, fruit.y, 1, 1, false, false)
		end
    end
end
