-- game

map_start_x = 0
grid_size_x = 6
grid_size_y = 6
offset_x = 0
offset_y = 0

function draw_hud()
	local score_msg = "score: "..score
	
	rectfill(62 - #score_msg * 2, 0, 64 + #score_msg * 2, 7, 0)
	rect(62 - #score_msg * 2, 0, 64 + #score_msg * 2, 7, 7)

    print(score_msg, 64 - #score_msg * 2, 2, 8)
    print(score_msg, 64 - #score_msg * 2, 1, 10)
end

function trigger_game_win()
    sfx(2)
	init_gamewin()
	cur_state = 3
end

function trigger_game_over()
	sfx(1)
	init_gameover()
	cur_state = 4
end

function init_game()
	destroy_list(cobra)
	last_dir = nil
	step_time = 0

	map_start_x = (map_size_index - 1) * 16
	offset_x = (flr((128 - (grid_size_x * 8)) / 2 / 8) * 8)
    offset_y = (flr((128 - (grid_size_y * 8)) / 2 / 8) * 8)

	init_cobra()
	init_fruit()
	score = 0

	sfx(3)
end

function update_game()
	update_cobra()
	update_fruit()
	if(cobra.size >= (grid_size_x * grid_size_y)) then
    	trigger_game_win()
	end
end

function draw_game()
	cls()
	map(map_start_x, 0)
	draw_cobra()
	draw_fruit()
	draw_hud()
end
